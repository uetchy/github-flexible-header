{
  "name": "__MSG_appName__",
  "version": "0.1.2",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "38": "icons/icon-38.png",
    "128": "icons/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "scripts/background.js",
      "scripts/chromereload.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [{
    "matches": [
      "http://github.com/*",
      "https://github.com/*"
    ],
    "js": [
      "scripts/contentscript.js"
    ],
    "run_at": "document_end"
  }]
}
